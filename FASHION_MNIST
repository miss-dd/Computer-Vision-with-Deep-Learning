        "def get_model():\n",
        "  model = Sequential()\n",
        "  model.add(Flatten(input_shape=(x_train.shape[1],)))\n",
        "  model.add(Dense(128, activation='relu'))\n",
        "  model.add(Dense(128, activation='relu'))\n",
        "  model.add(Dense(10, activation='softmax'))\n",
        "\n",
        "  return model"
